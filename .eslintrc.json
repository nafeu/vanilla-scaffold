{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-underscore-dangle": "off",
    "import/prefer-default-export": "off",
    "import/extensions": "off"
  },
  "plugins": ["prettier"],
  "overrides": [
    {
      "files": ["src/**/*.mjs"]
    }
  ]
}
